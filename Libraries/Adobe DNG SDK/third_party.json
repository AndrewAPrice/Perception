{
    "repositories" : [
        {
            "url": "https://download.adobe.com/pub/adobe/dng/dng_sdk_1_6.zip",
            "type": "zip",
            "placeholder": "zip"
        }
    ],
    "operations": [
        {
            "operation": "copy",
            "source": "${zip}/dng_sdk/source",
            "destination": "source",
            "extensions": [".cpp"]
        },
        {
            "operation": "copy",
            "source": "${zip}/dng_sdk/source",
            "destination": "public",
            "extensions": [".h"]
        }
    ]
}